{
  "expo": {
    "name": "Client",
    "slug": "whopaws",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tuempresa.client",
      "infoPlist": {
        "NSCameraUsageDescription": "Describe por qué necesitas acceso a la cámara",
        "NSPhotoLibraryUsageDescription": "Describe por qué necesitas acceso a la biblioteca de fotos",
        "NSMicrophoneUsageDescription": "Describe por qué necesitas acceso al micrófono"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",

      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "RECEIVE_BOOT_COMPLETED"
      ],

      "config": {
        "googleMaps": {
          "apiKey": "tu-clave-de-API-de-Google-Maps"
        }
      },
      "package": "com.tuempresa.client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "null",
          "enableGooglePay": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f26e932a-9a6a-4da8-9541-72c6153c7e81"
      }
    },
    "owner": "santy1422"
  }
}